{
  "name": "basic-express-server",
  "version": "1.0.0",
  "description": "express and typescript server bootstrap",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watchAll --verbose",
    "prettier": "npx prettier --write .",
    "lint": "eslint --ext .ts --config .eslintrc . --fix",
    "lint:fix": "npm run prettier && npm run lint",
    "build": "tsc",
    "serve": "tsc-watch --onSuccess \"node dist/index.js\"",
    "start": "npm run build && npm run serve",
    "start:prod": "npm run build && node dist/index.js"
  },
  "author": "joaquin gonzalez budino",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^29.1.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.1.2",
    "prettier": "2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.4"
  }
}
